# Portal_DTwins Environment Configuration
# Скопируйте в .env и заполните значения

# ============================================
# DATABASE (PostgreSQL + pgvector)
# ============================================
DB_HOST=localhost
DB_PORT=5432
DB_NAME=portal_dtwins
DB_USER=postgres
DB_PASSWORD=your_secure_password_here

# ============================================
# EMBEDDINGS (для семантического поиска)
# ============================================
EMBEDDING_PROVIDER=openai
EMBEDDING_MODEL=text-embedding-ada-002
EMBEDDING_DIMENSIONS=1536

# OpenAI API Key (для генерации embeddings)
OPENAI_API_KEY=sk-your-api-key-here

# ============================================
# APPLICATION
# ============================================
APP_ENV=development
LOG_LEVEL=INFO

# ============================================
# AGENT CONFIGURATION
# ============================================
AGENT_ID=AGENT-KNOWLEDGE-GATE
AGENT_SESSION_TTL=3600
